{
    "name": "patrickquijano/cakephp3-minifyhtml",
    "description": "MinifyHtml plugin for CakePHP 3",
    "type": "cakephp-plugin",
    "minimum-stability": "dev",
    "license": "MIT",
    "require": {
        "cakephp/cakephp": "^3.5",
        "wyrihaximus/html-compress": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.14|^6.0"
    },
    "autoload": {
        "psr-4": {
            "MinifyHtml\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MinifyHtml\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
